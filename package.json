  "scripts": {
    "build-watch": "npm run prebuild && npm run copy-dependencies && src/node_modules/.bin/tsc -w --preserveWatchOutput true",
    "watch": "NODE_ENV=dev webpack --watch --mode=development",
    "build": "NODE_ENV=prod webpack --mode=production",
    "test": "src/node_modules/.bin/mocha --require ts-node/register ./src/__tests__/**/*.spec.ts",
    "coverage": "npx nyc --reporter=text --reporter=html npm run test",
    "start-api": "src/node_modules/.bin/concurrently --kill-others --kill-others-on-fail \\\"npm run build-watch\\\" \\\"sam local start-api --template ../template.yml\\\""
  }
